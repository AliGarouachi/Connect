<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Employees" table="employees">
    <indexes>
      <index name="FK_Services" columns="IdService"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstName" type="string" column="First_Name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="Last_Name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cin" type="string" column="Cin" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="giveaccess" type="integer" column="GiveAccess" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="payment" type="integer" column="Payment" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="transfert" type="integer" column="Transfert" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="verifyaccess" type="integer" column="VerifyAccess" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idservice" target-entity="Services" fetch="LAZY">
      <join-columns>
        <join-column name="IdService" referenced-column-name="Id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
